{
  "manifest_version": 3,
  "name": "Drupico HTMX Browser Tutorial",
  "version": "0.1",
  "description": "Tutorial for using HTMX with Browser Extensions",
  "homepage_url": "http://127.0.0.1",
  "host_permissions": [
    "http://127.0.0.1/*"
  ],
  "icons": {
    "32": "icon_transparent_32x32.png"
  },
  "action": {
    "default_icon": "icon_transparent_32x32.png",
    "default_title": "Drupico",
    "default_popup": "popup.html"
  }
}
